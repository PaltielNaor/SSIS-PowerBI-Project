        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
		{
           

            var dataReviewResult = Dts.Variables["User::OnErrorMSG"].Value.ToString();

            var fromAddress = new MailAddress("paltiel.z@outlook.com", "SSIS Error On Dim Region Prosses " ); //from

            const string fromPassword = "India2014!"; // password
            var smtp = new SmtpClient
            {
                Host = "smtp-mail.outlook.com",
                Port = 587,//465,
                EnableSsl = true,
                DeliveryMethod = SmtpDeliveryMethod.Network,
                UseDefaultCredentials = false,
                Credentials = new NetworkCredential(fromAddress.Address, fromPassword),
                Timeout = 90000
            };

            MailMessage mailmsg = new MailMessage();
            mailmsg.IsBodyHtml = true;
            mailmsg.Subject = "SSIS Today Log Results";
            mailmsg.Body = dataReviewResult;
            mailmsg.From = new MailAddress(fromAddress.ToString());
            mailmsg.To.Add("<paltiel.z@outlook.com>");
            {
                try
                {
                    smtp.Send(mailmsg);
                }
                catch (Exception ex)
                {
                    MessageBox.Show(ex.Message);
                    //throw
                }   
            }

            // TODO: Add your code here

            Dts.TaskResult = (int)ScriptResults.Success;
		}

